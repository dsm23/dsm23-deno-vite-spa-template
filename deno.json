{
  "imports": {
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.7.0",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "vite": "npm:vite@^5.4.8"
  },
  "lint": {
    "exclude": [".vite", "dist", "node-modules"]
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run -A --unstable npm:vite",
    "build": "deno run -A --unstable npm:vite build",
    "preview": "deno run -A --unstable npm:vite preview",
    "serve": "deno run --allow-net --allow-read https://deno.land/std@0.157.0/http/file_server.ts dist/"
  }
}
